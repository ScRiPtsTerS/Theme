local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/ScRiPtsTerS/KavoTest/main/kavotest8"))()
local Notification = loadstring(game:HttpGet("https://raw.githubusercontent.com/Jxereas/UI-Libraries/main/notification_gui_library.lua", true))()
local Window = Library.CreateLib("Theme Selecter", "Midnight")
local Players = game.Players.LocalPlayer.Name

Notification.new("info", Players, "welcome to Theme Selecter")

---------------------------------------------------------------------------------------------------------------------------------------  
-- Functions

-- Destroy after Select theme
function Destroy()
    game:GetService("CoreGui").infinfinf:Destroy() -- infinfinf = name of gui
end

-- ThemeLoad
function ThemeLoad()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/ScRiPtsTerS/Test/main/Testing5"))()
end

-- Toggle Gui
function ToggleGui()
    -- Toggle Gui Window
    local Theme = Window:NewTab("Toggle UI")
    local MainTheme = Theme:NewSection("You can change settings")
    -- Toggle Gui Button
    MainToggle:NewKeybind("Toggle UI", "Self Explains", Enum.KeyCode.K, function()
        Library:ToggleUI()
    end)
end
---------------------------------------------------------------------------------------------------------------------------------------
-- Theme tab
    local Theme = Window:NewTab("Themes")
    local MainTheme = Theme:NewSection("Synapse Theme")
    -- Synapse Theme
        MainTheme:NewButton("Synapse GigaHub", "Synapse Theme", function()
        wait(0.25)
        Destroy()
        _G.THEMES = "Synapse"
        ThemeLoad()
        Notification.new("info", "Synapse theme", "loading  Synapse GigaHub!")
        end)

    -- Serpent Theme
