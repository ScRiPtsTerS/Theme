local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/ScRiPtsTerS/KavoTest/main/kavotest8"))()
local Notification = loadstring(game:HttpGet("https://raw.githubusercontent.com/Jxereas/UI-Libraries/main/notification_gui_library.lua", true))()
local Window = Library.CreateLib("Theme Selecter", "Midnight")
local Players = game.Players.LocalPlayer.Name

---------------------------------------------------------------------------------------------------------------------------------------  
-- Functions

-- Destroy after Select theme
function Destroy()
    game:GetService("CoreGui").infinfinf:Destroy() -- infinfinf = name of gui
end

-- ThemeLoad
function ThemeLoad()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/ScRiPtsTerS/Test/main/Testing6"))()
end

-- Premium Theme Load
function PremThemeLoad()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/ScRiPtsTerS/script/main/Premhub6"))()
end

-- Toggle Gui
function ToggleGui()
    -- Toggle Gui Window
    local Theme = Window:NewTab("Toggle UI")
    local MainTheme = Theme:NewSection("You can change settings")
    -- Toggle Gui Button
    MainTheme:NewKeybind("Toggle UI", "Self Explains", Enum.KeyCode.K, function()
        Library:ToggleUI()
    end)
end

function PrintTheme()
    print("Theme Selecter is loading")
    print("Made by ummidk(dripkage)#2417")
    wait(0.1)
    print("Loaded Synapse Theme")
    wait(0.4)
    print("Loaded Serpent theme")
    wait(0.2)
    print("Loaded Light Theme")
    wait(0.1)
    print("Loaded Dark Theme")
    wait(0.4)
    print("Loaded Grape Theme")
    wait(0.5)
    print("Loaded Blood Theme")
    wait(0.1)
    print("Loaded Ocean Theme")
    wait(0.3)
    print("Loaded Midnight Theme")
    wait(0.4)
    print("Loaded Sentinel Theme")
    wait(0.2)
    print("Loaded Orange Theme")
    wait(0.1)
    print("Loaded PremiumHub Theme")
    wait(0.5)
    wait(1)
    print("100%")
    print("ThemeSelecter has loaded")
end

function Kick(reason)
    game.Players.LocalPlayer:Kick(reason)
end
---------------------------------------------------------------------------------------------------------------------------------------

if _G.ThemeSelect == true then
    Notification.new("info", Players, "welcome to Theme Selecter", true, 3)
-- Theme tab
    local Theme = Window:NewTab("Themes")
    local MainTheme = Theme:NewSection("Synapse Theme")
    -- Synapse Theme
        MainTheme:NewButton("Synapse GigaHub", "Synapse Theme", function()
        wait(0.25)
        Destroy()
        Notification.new("info", "Synapse theme", "loading  Synapse GigaHub!", true, 3)
        _G.THEMES = "Synapse"
        ThemeLoad()
        end)

    -- Serpent Theme
        MainTheme:NewLabel("Serpent Theme")
        MainTheme:NewButton("Serpent GigaHub", "Serpent Theme", function()
        wait(0.25)
        Destroy()
        Notification.new("info", "Serpent theme", "loading Serpent GigaHub!", true, 3)
        _G.THEMES = "Serpent"
        ThemeLoad()
        end)

    -- Light Theme
        MainTheme:NewLabel("Light Theme")
        MainTheme:NewButton("Light GigaHub", "Light Theme", function()
        wait(0.25)
        Destroy()
        Notification.new("info", "Light theme", "loading Light GigaHub!", true, 3)
        _G.THEMES = "LightTheme"
        ThemeLoad()
        end)

    -- Dark Theme
        MainTheme:NewLabel("Dark Theme")
        MainTheme:NewButton("Dark GigaHub", "Dark Theme", function()
        wait(0.25)
        Destroy()
        Notification.new("info", "Dark theme", "loading Dark GigaHub!", true, 3)
        _G.THEMES = "DarkTheme"
        ThemeLoad()
        end)

    -- Grape Theme
        MainTheme:NewLabel("Grape Theme")
        MainTheme:NewButton("Grape GigaHub", "Grape Theme", function()
        wait(0.25)
        Destroy()
        Notification.new("info", "Grape theme", "loading Grape GigaHub!", true, 3)
        _G.THEMES = "GrapeTheme"
        ThemeLoad()
        end)

    -- Blood Theme
        MainTheme:NewLabel("Blood Theme")
        MainTheme:NewButton("Blood GigaHub", "Blood Theme", function()
        wait(0.25)
        Destroy()
        _G.THEMES = "BloodTheme"
        ThemeLoad()
        Notification.new("info", "Blood theme", "loading Blood GigaHub!", true, 3)
        end)

    -- Ocean Theme
        MainTheme:NewLabel("Ocean Theme")
        MainTheme:NewButton("Ocean GigaHub", "Ocean Theme", function()
        wait(0.25)
        Destroy()
        _G.THEMES = "Ocean"
        ThemeLoad()
        Notification.new("info", "Ocean theme", "loading Ocean GigaHub!", true, 3)
        end)

    -- Midnight Theme
        MainTheme:NewLabel("Midnight Theme")
        MainTheme:NewButton("Midnight GigaHub", "Midnight Theme", function()
        wait(0.25)
        Destroy()
        _G.THEMES = "Midnight"
        ThemeLoad()
        Notification.new("info", "Midnight theme", "loading Midnight GigaHub!", true, 3)
        end)

    -- Sentinel Theme
        MainTheme:NewLabel("Sentinel Theme")
        MainTheme:NewButton("Sentinel GigaHub", "Sentinel Theme", function()
        wait(0.25)
        Destroy()
        _G.THEMES = "Sentinel"
        ThemeLoad()
        Notification.new("info", "Sentinel theme", "loading Sentinel GigaHub!", true, 3)
        end)
        
    -- Orange Theme
        MainTheme:NewLabel("Orange Theme")
        MainTheme:NewButton("Orange GigaHub", "Orange Theme", function()
        wait(0.25)
        Destroy()
        _G.THEMES = "Orange"
        ThemeLoad()
        Notification.new("info", "Orange theme", "loading Orange GigaHub!", true, 3)
        end)
        
        -- Premium Theme
        MainTheme:NewLabel("Premium Theme 'needs key'")
        MainTheme:NewButton("Premium GigaHub", "Premium Theme", function()
        wait(0.25)
        Destroy()
        PremThemeLoad()
        Notification.new("info", "Premium theme", "loading Premium GigaHub!", true, 3)
        end)
        --ToggleGui
        ToggleGui()

        --Theme Print
        PrintTheme()
    elseif _G.ThemeSelect == false then
        Destroy()
        Destroy()
        _G.THEMES = _G.Theme
        Notification.new("info", _G.THEMES.." theme", "loading ".._G.THEMES.." GigaHub!", true, 3)
        if _G.THEMES == "Premhub" then
            PremThemeLoad()
        else
            ThemeLoad()
        end
    else
        Destroy()
        Destroy()
        Kick("Setting is not available for GigaHub. Try _G.ThemeSelect =  <- true of false")
    end
